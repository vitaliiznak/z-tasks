{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable"
    ],
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "./src",
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src"
  ]
}
