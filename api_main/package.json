    "express": "^4.17.1",
    "file-type-stream": "^1.0.0",
    "graphql": "^15.5.0",
    "graphql-constraint-directive": "^2.0.3",
    "graphql-scalars": "^1.9.3",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
